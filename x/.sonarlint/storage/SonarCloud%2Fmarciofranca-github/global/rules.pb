  if (i == 0) { // value of i was assigned after env was saved &amp; will be indeterminate after longjmp();
    // normal execution
  } else {
    // recover
  }
}

//...

void fun() {
  //...
  longjmp(buf, 1);  // Noncompliant
}
</pre>
<h2>Compliant Solution</h2>
